<%- include("partials/head") %>

<body>
  <%- include("partials/header") %>
  <main>
    <section class="container">
      <h1>Add Product content</h1>
      <form action="/process-data" method="POST">
        <div class="form-wrapper">
          <div class="form-group">
            <label for="pName" class="form-label">Product name</label>
            <input type="text" id="pName" name="productName" class="form-input" required>
          </div>
          <div class="form-group">
            <label for="pAge" class="form-label">Product number</label>
            <input type="number" id="pAge" name="productNum" class="form-input">
          </div>
        </div>
        <button type="submit" class="btn">Add Product</button>
      </form>
    </section>
  </main>
</body>
</html>
